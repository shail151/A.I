print("problema 01")
n=input("Dati numarul culorii curcubeului")
n=int(n)
if n==1:
    print("roÈ™u")
if n==2 :
    print("oranj")
if n==3 :
    print("galben")
if n==4 :
    print("verde")
if n==5 :
    print("albastru")
if n==6 :
    print("indigo")
if n==7 :
    print("violet")
if ((n>7) or (n<1)) :
    print ("Curcubeul are doar 7 culori")
